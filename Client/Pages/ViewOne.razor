                            </NotAuthorized>
                        </AuthorizeView>
                    </div>
                    <div class="d-flex justify-content-between">
                        <p class="text-decoration-underline">$786 x 5 nights</p>
                        <p>$3,929</p>
                    </div>
                    <div class="d-flex justify-content-between">
                        <p class="text-decoration-underline">Cleaning fee</p>
                        <p>$160</p>
                    </div>
                    <div class="d-flex justify-content-between border-bottom">
                        <p class="text-decoration-underline">Airbnb service fee</p>
                        <p>$577</p>
                    </div>
                    <div class="d-flex justify-content-between">
                        <p class="fw-bold">Total before taxes</p>
                        <p>$4,666</p>
                    </div>
                </EditForm>
            </div>
        </div>
    }
    <div id="map"></div>

    <!--
        The `defer` attribute causes the callback to execute after the full HTML
        document has been parsed. For non-blocking uses, avoiding race conditions,
        and consistent behavior across browsers, consider loading using Promises.
        See https://developers.google.com/maps/documentation/javascript/load-maps-js-api
        for more information.
        -->
</div>

@code {
    [Parameter]
    public int? id { get; set; } = null;
    RSVP booking = new RSVP();
    Property? p;
    protected async Task LoadData()
    {
        if (id == null) Nav.NavigateTo("/");
        var response = await Http.GetAsync($"api/Property/{id}");
        if (response.IsSuccessStatusCode)
        {
            p = await response.Content.ReadFromJsonAsync<Property>();
            booking.PropertyId = p.PropertyId;
            Console.WriteLine(p.PropertyId);
        }
    }
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await LoadData();
            await JSRuntime.InvokeVoidAsync("initMap", $"{p.Address}, {p.City} {p.Zip}");
            StateHasChanged();
        }
    }

    public async void HandleSubmit()
    {
        var response = await Http.PostAsJsonAsync("api/Payment/create-checkout-session", booking);
        if (response.IsSuccessStatusCode)
        {
            Nav.NavigateTo(await response.Content.ReadAsStringAsync());
        }
        else
        {
            Console.WriteLine("Something is Wrong");
        }
    }


}
